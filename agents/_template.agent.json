{
  "$schema": "./agent.schema.json",
  "schemaVersion": "2.0",
  "identity": {
    "id": "_template",
    "name": "New Agent",
    "title": "Role Title",
    "tagline": "A brief description of purpose",
    "avatar": "/avatars/default.png"
  },
  "role": "worker",
  "busConfig": {
    "canEmit": [
      "TASK_COMPLETE",
      "BLOCKER",
      "REQUEST_REVIEW"
    ],
    "canReceive": [
      "DISPATCH",
      "BROADCAST"
    ],
    "wakeEvents": [
      "DISPATCH"
    ]
  },
  "hardware": {
    "memory": {
      "type": "redis-vector",
      "retention": "permanent"
    },
    "voice": {
      "provider": "elevenlabs",
      "defaultVoiceId": "EXAVITQu4vr4xnSDxMaL"
    }
  },
  "skills": [
    "basic_chat",
    "web_search"
  ],
  "specialCapabilities": {
    "callRouting": {
      "canTransferTo": []
    }
  },
  "llmPolicy": {
    "allowedBackends": [
      "openai:gpt-5.2",
      "anthropic:claude-sonnet-4-20250514",
      "google:gemini-2.0-pro",
      "openrouter:deepseek-r1"
    ],
    "selection": {
      "primary": "openai:gpt-5.2",
      "secondary": "anthropic:claude-sonnet-4-20250514",
      "backup": "google:gemini-2.0-pro"
    }
  }
}